{
	"name": "time_of_day",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"format-check": "npx prettier --check .",
		"format-write": "npx prettier --write .",
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test --transformIgnorePatterns \"node_modules/(?!@toolz/allow-react)/\" --env=jsdom",
		"eject": "react-scripts eject",
		"api-test": "mocha \"src/tests/api tests/\"",
		"start-server": "node ./backend/server.js",
		"wait-backend": "./node_modules/.bin/wait-port localhost:8000",
		"wait-frontend": "./node_modules/.bin/wait-port localhost:3000",
		"start-app": "npm run start-server & npm run wait-backend && npm run start & npm run wait-frontend",
		"cy:open": "cypress open",
		"cy:open-and-serve": "npm run start-app & npm run cy:open",
		"cy:run": "cypress run",
		"cy:run-chrome": "cypress run --browser chrome",
		"cy:run-firefox": "cypress run --browser firefox",
		"cy:run-edge": "cypress run --browser edge",
		"cy:run-electron": "cypress run --browser electron",
		"cy:run-webkit": "cypress run --browser webkit",
		"cy:pipeline-run-chrome": "npm run start-app & npm run cy:run-chrome",
		"cy:pipeline-run-firefox": "npm run start-app & npm run cy:run-firefox",
		"cy:pipeline-run-edge": "npm run start-app & npm run cy:run-edge",
		"cy:pipeline-run-electron": "npm run start-app & npm run cy:run-electron",
		"cy:pipeline-run-webkit": "npm run start-app & npm run cy:run-webkit",
		"k6:pipeline-backend-run": "npm run start-server & npm run wait-backend && k6 run 'src/tests/performance tests/backend tests/loadTest.js'",
		"k6:pipeline-browser-home": "npm run start-app && k6 run 'src/tests/performance tests/browser tests/homePagePerformance.js'",
		"k6:pipeline-browser-todaystime": "npm run start-app && k6 run 'src/tests/performance tests/browser tests/todaysTimePagePerformance.js'",
		"k6:pipeline-browser-yourtime": "npm run start-app && k6 run 'src/tests/performance tests/browser tests/yourTimePagePerformance.js'"
	},
	"repository": {
		"type": "git",
